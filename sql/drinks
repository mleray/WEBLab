CREATE SEQUENCE public.drinks_id_seq INCREMENT BY 1 MINVALUE 1 MAXVALUE 9223372036854775807 START 1 CACHE 1;

CREATE TABLE public.drinks (
  id INTEGER PRIMARY KEY NOT NULL DEFAULT nextval('drinks_id_seq'::regclass),
  name CHARACTER(64) NOT NULL UNIQUE,
  volume INTEGER NOT NULL,
  alcohol DOUBLE PRECISION NOT NULL
);
